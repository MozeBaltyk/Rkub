{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base"
    ],
    "prHeader": "Renovate PR"
    "labels": ["dependencies"],
    "packageRules": [
      {
        "matchPackagePatterns": [
          "*"
        ],
        "matchUpdateTypes": [
          "minor",
          "patch"
        ],
        "groupName": "all non-major dependencies",
        "groupSlug": "all-minor-patch"
      }
    ]
  }
